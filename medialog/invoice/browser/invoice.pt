<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      i18n:domain="plone">
  <body>
    <div tal:repeat="medlem view/group_users">

      <div id="medlemmer">
        <p tal:content="medlem/fornavn|None" />
        <p tal:content="medlem/etternavn|None" />
        <p tal:content="medlem/adresse|None" />
        <p tal:content="medlem/postnr|None" />
        <p tal:content="medlem/poststed|None" />
      </div>

      <div id="bodytext">
         <p tal:replace="structure view/bodytext|None" />
      </div>

      <div id="sum">
        <p tal:content="view/sum|None">
      </div>

      <div id="kontonr">
        <p tal:content="view/kontonr|None">
      </div>

      <div id="paid_from">
        <p tal:content="medlem/fornavn|None" />
        <p tal:content="medlem/etternavn|None" />
        <p tal:content="medlem/adresse|None" />
        <p tal:content="medlem/postnr|None" />
        <p tal:content="medlem/poststed|None" />
      </div>

      <div id="paid_to">
        <div tal:replace="structure view/paid_to|None" />
      </div>

      <div id="total">
        <p tal:content="view/sum|None">
      </div>
    <div>
  </body>
</html>
